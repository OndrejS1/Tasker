source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!

target 'RainyShinyApp' do
    pod 'Alamofire', '~> 4.3'
    pod ‘Firebase'
	pod ‘Firebase/Core’
	pod ‘Firebase/AdMob’
	pod ‘Firebase/Messaging’
	pod ‘Firebase/Database’
	pod ‘Firebase/Auth’
	pod ‘Firebase/Storage’
  
end

